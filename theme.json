{
	"$schema": "https:\/\/schemas.wp.org\/trunk\/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"name": "Base",
					"slug": "base",
					"color": "#fff"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#217BF4"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#0A093D"
				},
				{
					"name": "Body",
					"slug": "body",
					"color": "#656464"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"name": "Inter",
					"slug": "inter",
					"fontFamily": "Inter, sans-serif",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/inter/Inter-Regular.woff",
								"file:./assets/fonts/inter/Inter-Regular.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/inter/Inter-Bold.woff",
								"file:./assets/fonts/inter/Inter-Bold.woff2"
							]
						}
					]
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)"
		}
	}
}
