{
	"$schema": "https:\/\/schemas.wp.org\/trunk\/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"color": {
			"palette": [
				{
					"name": "Granite Gray",
					"slug": "granite-gray",
					"color": "#656464"
				},
				{
					"name": "Cetacean Blue",
					"slug": "cetacean-blue",
					"color": "#0A093D"
				},
				{
					"name": "Blue (Crayola)",
					"slug": "blue-crayola",
					"color": "#217BF4"
				},
				{
					"name": "Bright Yellow (Crayola)",
					"slug": "bright-yellow-crayola",
					"color": "#FFAF2E"
				}
			],
			"gradients": [
				{
					"name": "Blue water to white",
					"slug": "blue-water-to-white",
					"gradient": "linear-gradient(135deg, #D4E7FE -15.24%, #FFFFFF 78.85%)"
				}
			]
		},
		"typography": {
			"fontSizes": [
				{
					"name": "Small",
					"slug": "sm",
					"size": "14px"
				},
				{
					"name": "Medium",
					"slug": "md",
					"size": "16px"
				},
				{
					"name": "Large",
					"slug": "lg",
					"size": "18px"
				},
				{
					"name": "X Large",
					"slug": "xl",
					"size": "26px"
				},
				{
					"name": "XX Large",
					"slug": "xxl",
					"size": "36px"
				},
				{
					"name": "XXX Large",
					"slug": "xxxl",
					"size": "52px"
				}
			],
			"fontFamilies": [
				{
					"name": "Inter",
					"slug": "inter",
					"fontFamily": "Inter"
				}
			]
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"layout": {
			"contentSize": "840px",
			"wideSize": "1170px"
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "var(--wp--preset--font-size--md)"
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "14px"
				},
				"color": {
					"background": "var(--wp--preset--color--blue-crayola)",
					"text": "#fff"
				},
				"border": {
					"radius": "14px"
				},
				"spacing": {
					"padding": {
						"top": "24px",
						"right": "42px",
						"bottom": "24px",
						"left": "42px"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxxl)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxl)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--md)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			}
		},
		"blocks": {
			"core/button": {
				"variations": {
					"fill": {
						"shadow": "0px 7px 22px -6px rgba(33, 123, 244, 0.34)"
					},
					"outline": {
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "1px"
						},
						"spacing": {
							"padding": {
								"top": "24px",
								"right": "42px",
								"bottom": "24px",
								"left": "42px"
							}
						}
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "10px"
				}
			}
		}
	}
}
